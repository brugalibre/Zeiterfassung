<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderPane" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.myownb3.dominic.ui.core.pages.overview.control.OverviewController">
   <bottom>
      <BorderPane BorderPane.alignment="CENTER">
         <bottom>
            <ButtonBar BorderPane.alignment="CENTER">
              <buttons>
                  <Button id="clearButton" fx:id="clearButton" mnemonicParsing="false" onAction="#onAction" text="clearButton" />
                <Button id="chargeOffButton" fx:id="bookButton" mnemonicParsing="false" onAction="#onAction" text="bookButton" />
                  <Button id="exportButton" fx:id="exportButton" mnemonicParsing="false" onAction="#onAction" text="exportButton" />
              </buttons>
               <BorderPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </BorderPane.margin>
            </ButtonBar>
         </bottom>
         <center>
            <ButtonBar BorderPane.alignment="CENTER">
              <buttons>
                  <Label fx:id="totalAmountOfTimeValue" text="Label" />
                  <Label fx:id="totalAmountOfTimeLabel" text="Label" />
              </buttons>
               <BorderPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </BorderPane.margin>
            </ButtonBar>
         </center>
      </BorderPane>
   </bottom>
   <top>
      <TableView fx:id="tableView" prefHeight="300.0" prefWidth="700.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
   </top>
</BorderPane>
