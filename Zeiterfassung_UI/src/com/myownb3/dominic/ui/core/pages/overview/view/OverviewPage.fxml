<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderPane" prefHeight="350.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.myownb3.dominic.ui.core.pages.overview.control.OverviewController">
   <padding>
      <Insets bottom="5.0" />
   </padding>
   <center>
      <VBox alignment="TOP_CENTER" spacing="5.0" BorderPane.alignment="TOP_CENTER">
         <children>
            <FlowPane hgap="5.0">
               <children>
                  <Label fx:id="totalAmountOfTimeLabel" text="Label" />
                  <Label fx:id="totalAmountOfTimeValue" text="Label" />
               </children>
            </FlowPane>
            <HBox prefHeight="30.0" spacing="5.0">
               <children>
                  <Button id="clearButton" fx:id="clearButton" mnemonicParsing="false" onAction="#onAction" text="clearButton" />
                <Button id="chargeOffButton" fx:id="bookButton" mnemonicParsing="false" onAction="#onAction" text="bookButton" />
                  <Button id="exportButton" fx:id="exportButton" mnemonicParsing="false" onAction="#onAction" text="exportButton" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <top>
      <TableView fx:id="tableView" maxHeight="275.0" prefHeight="275.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </top>
</BorderPane>
